<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
    	.dockLayoutPanel {
			position: static;
    	}
    	.southPanel {
			background-color: aqua;
    	}
		.northPanel {
			background-color: red;
		}
		.westPanel {
			background-color: yellow;
		}
		.centerPanel {
			background-color: gray;
		}
	</ui:style>
	<g:DockLayoutPanel unit='EM' styleName="{style.dockLayoutPanel}">
		<g:south size="5">
			<g:SimplePanel ui:field="south" styleName="{style.southPanel}"/>
		</g:south>
		<g:north size="5">
			<g:SimplePanel ui:field="north" styleName="{style.northPanel}"/>
		</g:north>
		<g:west size="15">
			<g:SimplePanel ui:field="west" styleName="{style.westPanel}"/>
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="center" styleName="{style.centerPanel}"/>
		</g:center>
		</g:DockLayoutPanel>
</ui:UiBinder>
